<p id="notice"><%= notice %></p>

<h1>Gifs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gifs.each do |gif| %>
      <p>
      </p>
      <tr>
        <%= gif.name %>
        <%= image_tag(url_for(gif.file), width: 200) %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gif', new_gif_path %>
